package unit;

public class Calculator {
		
		public static int findBig(int x,int y,int z)
		{
			if(x>y)
				return x;
			else
				return y;
		}
		
		public static int findBiggest(int x,int y,int z)
		{
			if(x>y && x>z)
				return x;
			else if(y>x && y>z)
				return y;
			else
				return z;
		}
		

	}



